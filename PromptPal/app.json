{
  "expo": {
    "name": "PromptPal",
    "slug": "PromptPal",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "scheme": "promptpal",
    "splash": {
      "image": "./assets/images/image-gen.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/images/image-gen.png",
      "bundler": "metro",
      "build": {
        "babel": {
          "include": ["nativewind"]
        }
      }
    },
    "plugins": ["expo-router"]
  }
}
